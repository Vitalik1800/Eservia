<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <item name="match_parent" type="dimen" tools:ignore="MissingDefaultResource">-1</item>
    <item name="wrap_content" type="dimen" tools:ignore="MissingDefaultResource">-2</item>

    <attr name="acceptFormButtonStyle" format="reference" tools:ignore="MissingDefaultResource" />
    <attr name="bookItemStyle" format="reference" tools:ignore="MissingDefaultResource" />
    <attr name="hourItemStyle" format="reference" tools:ignore="MissingDefaultResource" />

    <declare-styleable name="PlaceHolderLayout">
        <attr name="icon" format="reference" />
        <attr name="text" format="string|reference" />
        <attr name="buttonText" format="string|reference" />
        <attr name="empty" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CommonPlaceHolder">
        <attr name="iconEmpty"  format="reference" />
        <attr name="textEmpty" format="string|reference" />
        <attr name="iconNoConnection" format="reference" />
        <attr name="textNoConnection" format="string|reference" />
        <attr name="iconUnknownError" format="reference" />
        <attr name="textUnknownError" format="string|reference" />
        <attr name="state" format="integer" />
        <attr name="textButton" format="string|reference" />
        <attr name="stateButton" format="integer" />
    </declare-styleable>
</resources>
